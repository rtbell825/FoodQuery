<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="325.0" prefWidth="229.0" style="-fx-background-color: #636363;" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AnalyzeMealController">
   <children>
      <VBox prefHeight="350.0" prefWidth="229.0">
         <children>
            <Label text="Calories" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="88.0" />
               </padding>
               <VBox.margin>
                  <Insets top="35.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="TotalCaloris">
               <VBox.margin>
                  <Insets left="60.0" right="60.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label text="Fat(grams)" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="80.0" />
               </padding>
               <VBox.margin>
                  <Insets top="2.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="TotalFat">
               <VBox.margin>
                  <Insets left="60.0" right="60.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label text="Carbohydrates(grams)" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="40.0" />
               </padding>
               <VBox.margin>
                  <Insets top="2.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="TotalCarbs" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="60.0" right="60.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label text="Fiber(grams)" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="75.0" />
               </padding>
               <VBox.margin>
                  <Insets top="2.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="TotalFiber" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="60.0" right="60.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label text="Protein(grams)" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="70.0" />
               </padding>
               <VBox.margin>
                  <Insets top="2.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="TotalProtein" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="60.0" right="60.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="AnalyzeTotalMeal" mnemonicParsing="false" onAction="#totalNutrientsInMeal" style="-fx-background-color: #C5050C#C5050C;" text="Analyze" textFill="WHITE">
               <VBox.margin>
                  <Insets left="80.0" top="15.0" />
               </VBox.margin>
               <effect>
                  <DropShadow blurType="ONE_PASS_BOX" />
               </effect>
            </Button>
         </children>
      </VBox>
      <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <menus>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
